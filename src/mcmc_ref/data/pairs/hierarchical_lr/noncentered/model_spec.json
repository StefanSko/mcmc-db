{
  "parameters": [
    {"name": "mu_alpha", "shape": [], "constraint": "real"},
    {"name": "sigma_alpha", "shape": [], "constraint": "lower=0"},
    {"name": "alpha_raw", "shape": [6], "constraint": "real"},
    {"name": "beta", "shape": [], "constraint": "real"},
    {"name": "sigma_y", "shape": [], "constraint": "lower=0"}
  ],
  "transformed_parameters": [
    {"name": "alpha", "shape": [6], "expression": "mu_alpha + sigma_alpha * alpha_raw"}
  ],
  "data": [
    {"name": "N", "type": "int"},
    {"name": "K", "type": "int"},
    {"name": "group", "type": "int", "shape": ["N"]},
    {"name": "x", "type": "real", "shape": ["N"]},
    {"name": "y", "type": "real", "shape": ["N"]}
  ],
  "log_density_components": [
    {"type": "normal", "variate": "mu_alpha", "mean": 0, "sd": 10},
    {"type": "half_normal", "variate": "sigma_alpha", "sd": 5},
    {"type": "normal", "variate": "alpha_raw", "mean": 0, "sd": 1},
    {"type": "normal", "variate": "beta", "mean": 0, "sd": 5},
    {"type": "half_normal", "variate": "sigma_y", "sd": 5},
    {"type": "normal", "variate": "y", "mean": "alpha[group] + beta * x", "sd": "sigma_y"}
  ]
}
